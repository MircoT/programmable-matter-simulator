fmt := import("fmt")

transition := func(state, N1, N2, N1deg) {
    if state != "CONTRACTED" {
        if state == "EXPANDEDR" {
            return "MOVER"
        } else if state == "EXPANDEDL" {
            return "MOVEL"
        }
    }

    numN1 := 0
    for n in N1 {
        if n != "VOID" {
            numN1 += 1
        }
    }
    fmt.println(state, " ", numN1, " ", N1, " ", N1[0] == "VOID", " ", N1[1] == "VOID")

    if numN1 > 0 {
        if N1[0] == "VOID" {
            return "EXPANDEDL"
        } else if N1[1] == "VOID" {
             return "EXPANDEDR"
        } else {
            return "CONTRACTED"
        }
    }

    return state
}
